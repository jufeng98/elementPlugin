{
  "name": "el-dropdown",
  "attributes": [
    {
      "name": "type",
      "desc": "菜单按钮类型，同 Button 组件(只在`split-button`为 true 的情况下有效)",
      "type": "string",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "size",
      "desc": "菜单尺寸，在`split-button`为 true 的情况下也对触发按钮生效",
      "type": "string",
      "optionValue": "medium / small / mini",
      "options": [
        "medium",
        "small",
        "mini"
      ],
      "defaultValue": "—"
    },
    {
      "name": "split-button",
      "desc": "下拉触发元素呈现为按钮组",
      "type": "boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "placement",
      "desc": "菜单弹出位置",
      "type": "string",
      "optionValue": "top/top-start/top-end/bottom/bottom-start/bottom-end",
      "options": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end"
      ],
      "defaultValue": "bottom-end"
    },
    {
      "name": "trigger",
      "desc": "触发下拉的行为",
      "type": "string",
      "optionValue": "hover, click",
      "options": [],
      "defaultValue": "hover"
    },
    {
      "name": "hide-on-click",
      "desc": "是否在点击菜单项后隐藏菜单",
      "type": "boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "show-timeout",
      "desc": "展开下拉菜单的延时（仅在 trigger 为 hover 时有效）",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "250"
    },
    {
      "name": "hide-timeout",
      "desc": "收起下拉菜单的延时（仅在 trigger 为 hover 时有效）",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "150"
    },
    {
      "name": "tabindex",
      "desc": "<p>Dropdown 组件的 <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Global_attributes/tabindex\">tabindex</a></p>",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "disabled",
      "desc": "是否禁用",
      "type": "boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [
    {
      "name": "—",
      "desc": "触发下拉列表显示的元素。 注意： 必须是一个元素或者或者组件"
    },
    {
      "name": "dropdown",
      "desc": "下拉列表，通常是 `<el-dropdown-menu>` 组件"
    }
  ],
  "events": [
    {
      "name": "@click",
      "desc": "`split-button` 为 true 时，点击左侧按钮的回调",
      "param": "—"
    },
    {
      "name": "@command",
      "desc": "点击菜单项触发的事件回调",
      "param": "dropdown-item 的指令"
    },
    {
      "name": "@visible-change",
      "desc": "下拉框出现/隐藏时触发",
      "param": "出现则为 true，隐藏则为 false"
    }
  ],
  "methods": []
}