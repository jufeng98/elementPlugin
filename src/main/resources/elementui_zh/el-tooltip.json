{
  "name": "el-tooltip",
  "attributes": [
    {
      "name": "effect",
      "desc": "默认提供的主题",
      "type": "String",
      "optionValue": "dark/light",
      "options": [
        "dark",
        "light"
      ],
      "defaultValue": "dark"
    },
    {
      "name": "content",
      "desc": "显示的内容，也可以通过 `slot#content` 传入 DOM",
      "type": "String",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "placement",
      "desc": "Tooltip 的出现位置",
      "type": "String",
      "optionValue": "top/top-start/top-end/bottom/bottom-start/bottom-end/left/left-start/left-end/right/right-start/right-end",
      "options": [
        "top",
        "top-start",
        "top-end",
        "bottom",
        "bottom-start",
        "bottom-end",
        "left",
        "left-start",
        "left-end",
        "right",
        "right-start",
        "right-end"
      ],
      "defaultValue": "bottom"
    },
    {
      "name": "value",
      "desc": "状态是否可见",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "v-model",
      "desc": "状态是否可见",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "disabled",
      "desc": "Tooltip 是否可用",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "offset",
      "desc": "出现位置的偏移量",
      "type": "Number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "transition",
      "desc": "定义渐变动画",
      "type": "String",
      "optionValue": "—",
      "options": [],
      "defaultValue": "el-fade-in-linear"
    },
    {
      "name": "visible-arrow",
      "desc": "<p>是否显示 Tooltip 箭头，更多参数可见<a href=\"https://github.com/element-component/vue-popper\">Vue-popper</a></p>",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "popper-options",
      "desc": "[popper.js](https://popper.js.org/docs/v2/) 的参数",
      "type": "Object",
      "optionValue": "<p>参考 <a href=\"https://popper.js.org/docs/v2/\">popper.js</a> 文档</p>",
      "options": [],
      "defaultValue": "{ boundariesElement: 'body', gpuAcceleration: false }"
    },
    {
      "name": "open-delay",
      "desc": "延迟出现，单位毫秒",
      "type": "Number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "manual",
      "desc": "手动控制模式，设置为 true 后，mouseenter 和 mouseleave 事件将不会生效",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "popper-class",
      "desc": "为 Tooltip 的 popper 添加类名",
      "type": "String",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "enterable",
      "desc": "鼠标是否可进入到 tooltip 中",
      "type": "Boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "hide-after",
      "desc": "Tooltip 出现后自动隐藏延时，单位毫秒，为 0 则不会自动隐藏",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "tabindex",
      "desc": "<p>Tooltip 组件的 <a href=\"https://developer.mozilla.org/en-US/docs/Web/HTML/Global_attributes/tabindex\">tabindex</a></p>",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "0"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [],
  "events": [],
  "methods": []
}