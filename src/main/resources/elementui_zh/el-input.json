    {
      "name": "@blur",
      "desc": "在 Input 失去焦点时触发",
      "param": "(event: Event)"
    },
    {
      "name": "@focus",
      "desc": "在 Input 获得焦点时触发",
      "param": "(event: Event)"
    },
    {
      "name": "@change",
      "desc": "仅在输入框失去焦点或用户按下回车时触发",
      "param": "(value: string \\| number)"
    },
    {
      "name": "@input",
      "desc": "在 Input 值改变时触发",
      "param": "(value: string \\| number)"
    },
    {
      "name": "@clear",
      "desc": "在点击由 `clearable` 属性生成的清空按钮时触发",
      "param": "—"
    }
  ],
  "methods": [
    {
      "name": "focus",
      "desc": "使 input 获取焦点",
      "param": "—"
    },
    {
      "name": "blur",
      "desc": "使 input 失去焦点",
      "param": "—"
