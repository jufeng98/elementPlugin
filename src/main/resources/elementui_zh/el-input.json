    {
      "name": "prefix",
      "desc": "输入框头部内容，只对 `type=\"text\"` 有效"
    },
    {
      "name": "suffix",
      "desc": "输入框尾部内容，只对 `type=\"text\"` 有效"
    },
    {
      "name": "prepend",
      "desc": "输入框前置内容，只对 `type=\"text\"` 有效"
