{
  "name": "el-progress",
  "attributes": [
    {
      "name": "**percentage**",
      "desc": "percentage, **required**",
      "type": "number",
      "optionValue": "0-100",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "type",
      "desc": "the type of progress bar",
      "type": "string",
      "optionValue": "line/circle/dashboard",
      "options": [
        "line",
        "circle",
        "dashboard"
      ],
      "defaultValue": "line"
    },
    {
      "name": "stroke-width",
      "desc": "the width of progress bar",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "6"
    },
    {
      "name": "text-inside",
      "desc": "whether to place the percentage inside progress bar, only works when `type` is 'line'",
      "type": "boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "status",
      "desc": "the current status of progress bar",
      "type": "string",
      "optionValue": "success/exception/warning",
      "options": [
        "success",
        "exception",
        "warning"
      ],
      "defaultValue": "—"
    },
    {
      "name": "color",
      "desc": "background color of progress bar. Overrides `status` prop",
      "type": "string/function/array",
      "optionValue": "—",
      "options": [],
      "defaultValue": "''"
    },
    {
      "name": "width",
      "desc": "the canvas width of circle progress bar",
      "type": "number",
      "optionValue": "—",
      "options": [],
      "defaultValue": "126"
    },
    {
      "name": "show-text",
      "desc": "whether to show percentage",
      "type": "boolean",
      "optionValue": "—",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "stroke-linecap",
      "desc": "circle/dashboard type shape at the end path",
      "type": "string",
      "optionValue": "butt/round/square",
      "options": [
        "butt",
        "round",
        "square"
      ],
      "defaultValue": "round"
    },
    {
      "name": "format",
      "desc": "custom text format",
      "type": "function(percentage)",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "define-back-color",
      "desc": "background color of progress bar (hex format)",
      "type": "string",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "text-color",
      "desc": "text color of progress bar (hex format)",
      "type": "string",
      "optionValue": "—",
      "options": [],
      "defaultValue": "—"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [],
  "events": [],
  "methods": []
}