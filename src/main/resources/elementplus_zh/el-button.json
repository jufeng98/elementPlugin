{
  "name": "el-button",
  "attributes": [
    {
      "name": "size",
      "desc": "尺寸",
      "type": "enum",
      "optionValue": "large | default | small",
      "options": [
        "large",
        "default",
        "small"
      ],
      "defaultValue": "—"
    },
    {
      "name": "type",
      "desc": "按钮类型，在设置color时，后者优先。",
      "type": "enum",
      "optionValue": "primary | success | warning | danger | info | text (deprecated)",
      "options": [
        "primary",
        "success",
        "warning",
        "danger",
        "info",
        "text (deprecated)"
      ],
      "defaultValue": "—"
    },
    {
      "name": "plain",
      "desc": "是否为朴素按钮",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "text",
      "desc": "是否为文字按钮(2.2.0)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "bg",
      "desc": "是否显示文字按钮背景颜色(2.2.0)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "link",
      "desc": "是否为链接按钮(2.2.1)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "round",
      "desc": "是否为圆角按钮",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "circle",
      "desc": "是否为圆形按钮",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "loading",
      "desc": "是否为加载中状态",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "loading-icon",
      "desc": "自定义加载中状态图标组件",
      "type": "string / Component",
      "optionValue": "",
      "options": [],
      "defaultValue": "Loading"
    },
    {
      "name": "disabled",
      "desc": "按钮是否为禁用状态",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "icon",
      "desc": "图标组件",
      "type": "string / Component",
      "optionValue": "",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "autofocus",
      "desc": "原生 autofocus 属性",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "native-type",
      "desc": "原生 type 属性",
      "type": "enum",
      "optionValue": "button | submit | reset",
      "options": [
        "button",
        "submit",
        "reset"
      ],
      "defaultValue": "button"
    },
    {
      "name": "auto-insert-space",
      "desc": "两个中文字符之间自动插入空格(仅当文本长度为 2 且所有字符均为中文时才生效)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "color",
      "desc": "自定义按钮颜色, 并自动计算 hover 和 active 触发后的颜色",
      "type": "string",
      "optionValue": "",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "dark",
      "desc": "dark 模式, 意味着自动设置 color 为 dark 模式的颜色",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "tag",
      "desc": "自定义元素标签(2.3.4)",
      "type": "string / Component",
      "optionValue": "",
      "options": [],
      "defaultValue": "button"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [
    {
      "name": "default",
      "desc": "自定义默认内容"
    },
    {
      "name": "loading",
      "desc": "自定义加载中组件"
    },
    {
      "name": "icon",
      "desc": "自定义图标组件"
    }
  ],
  "events": [],
  "methods": [
    {
      "name": "ref",
      "desc": "按钮 html 元素",
      "param": "objectRef<HTMLButtonElement>"
    },
    {
      "name": "size",
      "desc": "按钮尺寸",
      "param": "objectComputedRef< | small | default | large>"
    },
    {
      "name": "type",
      "desc": "按钮类型",
      "param": "objectComputedRef< | default | primary | success | warning | info | danger | text>"
    },
    {
      "name": "disabled",
      "desc": "按钮已禁用",
      "param": "objectComputedRef<boolean>"
    },
    {
      "name": "shouldAddSpace",
      "desc": "是否在两个字符之间插入空格",
      "param": "objectComputedRef<boolean>"
    }
  ]
}