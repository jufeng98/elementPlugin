{
  "name": "el-progress",
  "attributes": [
    {
      "name": "percentage",
      "desc": "percentage(required)",
      "type": "number",
      "optionValue": "(0-100)",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "type",
      "desc": "进度条类型",
      "type": "enum",
      "optionValue": "line | circle | dashboard",
      "options": [
        "line",
        "circle",
        "dashboard"
      ],
      "defaultValue": "line"
    },
    {
      "name": "stroke-width",
      "desc": "进度条的宽度",
      "type": "number",
      "optionValue": "",
      "options": [],
      "defaultValue": "6"
    },
    {
      "name": "text-inside",
      "desc": "进度条显示文字内置在进度条内（仅 type 为 'line' 时可用）",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "status",
      "desc": "进度条当前状态",
      "type": "enum",
      "optionValue": "success | exception | warning",
      "options": [
        "success",
        "exception",
        "warning"
      ],
      "defaultValue": "—"
    },
    {
      "name": "indeterminate",
      "desc": "是否为动画进度条",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "duration",
      "desc": "控制动画进度条速度和条纹进度条流动速度",
      "type": "number",
      "optionValue": "",
      "options": [],
      "defaultValue": "3"
    },
    {
      "name": "color",
      "desc": "进度条背景色 （会覆盖 status 状态颜色）",
      "type": "string / function",
      "optionValue": "(percentage: number) => string",
      "options": [],
      "defaultValue": ""
    },
    {
      "name": "width",
      "desc": "环形进度条画布宽度（只在 type 为 circle 或 dashboard 时可用）",
      "type": "number",
      "optionValue": "",
      "options": [],
      "defaultValue": "126"
    },
    {
      "name": "show-text",
      "desc": "是否显示进度条文字内容",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "stroke-linecap",
      "desc": "circle/dashboard 类型路径两端的形状",
      "type": "enum",
      "optionValue": "butt | round | square",
      "options": [
        "butt",
        "round",
        "square"
      ],
      "defaultValue": "round"
    },
    {
      "name": "format",
      "desc": "指定进度条文字内容",
      "type": "Function",
      "optionValue": "(percentage: number) => string",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "striped",
      "desc": "在进度条上增加条纹(2.3.4)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "striped-flow",
      "desc": "让进度条上的条纹流动起来(2.3.4)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [
    {
      "name": "default",
      "desc": "自定义内容"
    }
  ],
  "events": [],
  "methods": []
}