{
  "name": "el-transfer",
  "attributes": [
    {
      "name": "model-value",
      "desc": "选中项绑定值",
      "type": "object",
      "optionValue": "Array<string | number>",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "v-model",
      "desc": "选中项绑定值",
      "type": "object",
      "optionValue": "Array<string | number>",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "data",
      "desc": "Transfer 的数据源",
      "type": "object",
      "optionValue": "Record<string, any>[]",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "filterable",
      "desc": "是否可搜索",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "filter-placeholder",
      "desc": "搜索框占位符",
      "type": "string",
      "optionValue": "",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "filter-method",
      "desc": "自定义搜索方法",
      "type": "Function",
      "optionValue": "(query: string, item: Record<string, any>) => boolean",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "target-order",
      "desc": "右侧列表元素的排序策略： 若为 original，则保持与数据源相同的顺序； 若为 push，则新加入的元素排在最后； 若为 unshift，则新加入的元素排在最前",
      "type": "enum",
      "optionValue": "original | push | unshift",
      "options": [
        "original",
        "push",
        "unshift"
      ],
      "defaultValue": "original"
    },
    {
      "name": "titles",
      "desc": "自定义列表标题",
      "type": "object",
      "optionValue": "[string, string]",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "button-texts",
      "desc": "自定义按钮文案",
      "type": "object",
      "optionValue": "[string, string]",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "render-content",
      "desc": "自定义数据项渲染函数",
      "type": "object",
      "optionValue": "renderContent",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "format",
      "desc": "列表顶部勾选状态文案",
      "type": "object",
      "optionValue": "TransferFormat",
      "options": [],
      "defaultValue": "{}"
    },
    {
      "name": "[props](#type-declarations)",
      "desc": "数据源的字段别名",
      "type": "object",
      "optionValue": "TransferPropsAlias",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "left-default-checked",
      "desc": "初始状态下左侧列表的已勾选项的 key 数组",
      "type": "object",
      "optionValue": "Array<string | number>",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "right-default-checked",
      "desc": "初始状态下右侧列表的已勾选项的 key 数组",
      "type": "object",
      "optionValue": "Array<string | number>",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "validate-event",
      "desc": "是否触发表单验证",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "true"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [
    {
      "name": "default",
      "desc": "自定义数据项的内容，"
    },
    {
      "name": "left-footer",
      "desc": "左侧列表底部的内容"
    },
    {
      "name": "right-footer",
      "desc": "右侧列表底部的内容"
    },
    {
      "name": "left-empty ^(2.9.0)",
      "desc": "左侧面板为空或没有数据符合筛选条件时的内容"
    },
    {
      "name": "right-empty ^(2.9.0)",
      "desc": "右侧面板为空或没有数据符合筛选条件时的内容"
    }
  ],
  "events": [
    {
      "name": "@change",
      "desc": "右侧列表元素变化时触发",
      "param": "Function(value: TransferKey[, direction: TransferDirection, movedKeys: TransferKey[) => void"
    },
    {
      "name": "@left-check-change",
      "desc": "左侧列表元素被用户选中 / 取消选中时触发",
      "param": "Function(value: TransferKey[, movedKeys?: TransferKey[) => void"
    },
    {
      "name": "@right-check-change",
      "desc": "右侧列表元素被用户选中 / 取消选中时触发",
      "param": "Function(value: TransferKey[, movedKeys?: TransferKey[) => void"
    }
  ],
  "methods": [
    {
      "name": "clearQuery",
      "desc": "清空某个面板的搜索关键词",
      "param": "Function(which: TransferDirection) => void"
    },
    {
      "name": "leftPanel",
      "desc": "左侧面板 ref",
      "param": "objectRef<TransferPanelInstance>"
    },
    {
      "name": "rightPanel",
      "desc": "右侧面板 ref",
      "param": "objectRef<TransferPanelInstance>"
    }
  ]
}