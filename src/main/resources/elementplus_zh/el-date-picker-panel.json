{
  "name": "el-date-picker-panel",
  "attributes": [
    {
      "name": "model-value",
      "desc": "绑定值，如果是 range 选择器，数组长度应为 2",
      "type": "number / string / Date / array",
      "optionValue": "number[] | string[] | Date[]",
      "options": [
        "number[]",
        "string[]",
        "Date[]"
      ],
      "defaultValue": ""
    },
    {
      "name": "v-model",
      "desc": "绑定值，如果是 range 选择器，数组长度应为 2",
      "type": "number / string / Date / array",
      "optionValue": "number[] | string[] | Date[]",
      "options": [
        "number[]",
        "string[]",
        "Date[]"
      ],
      "defaultValue": ""
    },
    {
      "name": "border",
      "desc": "日期选择器是否有边框",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "disabled",
      "desc": "禁用",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "clearable",
      "desc": "是否显示清除按钮",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "true"
    },
    {
      "name": "type",
      "desc": "选择器类型，默认是普通选择器",
      "type": "enum",
      "optionValue": "year | years |month | months | date | dates | datetime | week | datetimerange | daterange | monthrange | yearrange",
      "options": [
        "year",
        "years",
        "month",
        "months",
        "date",
        "dates",
        "datetime",
        "week",
        "datetimerange",
        "daterange",
        "monthrange",
        "yearrange"
      ],
      "defaultValue": "date"
    },
    {
      "name": "default-value",
      "desc": "可选，选择器打开时默认显示的时间",
      "type": "object",
      "optionValue": "Date | [Date, Date]",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "default-time",
      "desc": "范围选择时选中日期所使用的当日内具体时刻",
      "type": "object",
      "optionValue": "Date | [Date, Date]",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "value-format",
      "desc": "可选，绑定值的格式。 不指定则绑定值为 Date 对象",
      "type": "string",
      "optionValue": "",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "date-format",
      "desc": "可选，输入框内部面板中显示的日期格式",
      "type": "<p>string see [date formats(https://day.js.org/docs/en/display/format)</p>",
      "optionValue": "",
      "options": [],
      "defaultValue": "YYYY-MM-DD"
    },
    {
      "name": "time-format",
      "desc": "可选，输入框内部面板中显示的时间格式",
      "type": "<p>string see [date formats(https://day.js.org/docs/en/display/format)</p>",
      "optionValue": "",
      "options": [],
      "defaultValue": "HH:mm:ss"
    },
    {
      "name": "unlink-panels",
      "desc": "在范围选择器里取消两个日期面板之间的联动",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "disabled-date",
      "desc": "一个用来判断该日期是否被禁用的函数，接受一个 Date 对象作为参数。 应该返回一个 Boolean 值。",
      "type": "Function",
      "optionValue": "(data: Date) => boolean",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "shortcuts",
      "desc": "设置快捷选项，需要传入数组对象",
      "type": "object",
      "optionValue": "Array<{ text: string, value: Date | Function }>",
      "options": [],
      "defaultValue": "[]"
    },
    {
      "name": "cell-class-name",
      "desc": "设置自定义类名",
      "type": "Function",
      "optionValue": "(data: Date) => string",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "show-footer",
      "desc": "是否在日期选择器中显示页脚 ^\\[enum]'dates' \\\\| 'months' \\\\| 'years' \\\\| 'datetime' \\\\| 'datetimerange'",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "show-confirm",
      "desc": "是否显示确定按钮",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "show-week-number",
      "desc": "显示周数(除周外)",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    }
  ],
  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [
    {
      "name": "default",
      "desc": "自定义单元格内容"
    },
    {
      "name": "prev-month",
      "desc": "上个月的图标"
    },
    {
      "name": "next-month",
      "desc": "下个月的图标"
    },
    {
      "name": "prev-year",
      "desc": "上一年图标"
    },
    {
      "name": "next-year",
      "desc": "下一年图标"
    }
  ],
  "events": [
    {
      "name": "@calendar-change",
      "desc": "在日历所选日期更改时触发 仅限“range”类型。",
      "param": "Function(val: [Date, null | Date) => void"
    },
    {
      "name": "@panel-change",
      "desc": "当日期面板改变时触发。",
      "param": "Function(date: Date | [Date, Date, mode: month | year, view?: string) => void"
    }
  ],
  "methods": []
}