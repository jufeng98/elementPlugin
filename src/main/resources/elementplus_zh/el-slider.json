  "props": [],
  "options": [],
  "shortcuts": [],
  "slots": [],
  "events": [
    {
      "name": "@change",
      "desc": "值改变时触发（使用鼠标拖曳时，只在松开鼠标后触发）",
      "param": "Function(value: Arrayable<number>) => boolean"
    },
    {
      "name": "@input",
      "desc": "数据改变时触发（使用鼠标拖曳时，活动过程实时触发）",
      "param": "Function(value: Arrayable<number>) => boolean"
    }
  ],
  "methods": []
