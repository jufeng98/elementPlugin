      "optionValue": " | large | default | small",
      "options": [
        "large",
        "default",
        "small"
      ],
      "defaultValue": "default"
    },
    {
      "name": "input-size",
      "desc": "输入框的大小，如果设置了 size 属性，默认值自动取 size",
      "type": "enum",
      "optionValue": " | large | default | small",
      "options": [
        "large",
        "default",
        "small"
      ],
      "defaultValue": "default"
    },
    {
      "name": "show-stops",
      "desc": "是否显示间断点",
      "type": "boolean",
      "optionValue": "",
