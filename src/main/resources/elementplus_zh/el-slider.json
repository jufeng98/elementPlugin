    },
    {
      "name": "vertical",
      "desc": "垂直模式",
      "type": "boolean",
      "optionValue": "",
      "options": [],
      "defaultValue": "false"
    },
    {
      "name": "height",
      "desc": "滑块高度，垂直模式必填",
      "type": "string",
      "optionValue": "",
      "options": [],
