    {
      "name": "model-value",
      "desc": "选中项绑定值",
      "type": "number / object",
      "optionValue": "number[]",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "v-model",
      "desc": "选中项绑定值",
      "type": "number / object",
      "optionValue": "number[]",
      "options": [],
      "defaultValue": "0"
    },
    {
      "name": "min",
      "desc": "最小值",
      "type": "number",
      "optionValue": "",
      "options": [],
      "defaultValue": "0"
