    {
      "name": "aria-label",
      "desc": "原生 aria-label属性(a11y) (2.7.2)",
      "type": "string",
      "optionValue": "",
      "options": [],
      "defaultValue": "—"
    },
    {
      "name": "range-start-label",
      "desc": "当 range 为true时，屏幕阅读器标签开始的标记",
      "type": "string",
      "optionValue": "",
      "options": [],
